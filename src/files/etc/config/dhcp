config dnsmasq 'invizbox'
	option authoritative '1'
	option domain 'lan'
	option domainneeded '1'
	option expandhosts '1'
	list interface 'loopback'
	option leasefile '/tmp/dhcp.leases.ib2'
	option local '/lan/'
	option localise_queries '1'
	option localservice '1'
	option localuse '1'
	option readethers '1'
	list rebind_domain 'onion'
	list rebind_domain 'vpn1'
	list rebind_domain 'vpn2'
	list rebind_domain 'vpn3'
	list rebind_domain 'vpn4'
	list rebind_domain 'tor'
	list rebind_domain 'clear1'
	list rebind_domain 'clear2'
	list rebind_domain 'loc'
	option rebind_localhost '1'
	option resolvfile '/tmp/resolv.conf.auto'
	list server '/vpn1/10.154.0.1'
	list server '/vpn2/10.154.1.1'
	list server '/vpn3/10.154.2.1'
	list server '/vpn4/10.154.3.1'
	list server '/tor/10.154.4.1'
	list server '/clear1/10.154.5.1'
	list server '/clear2/10.154.6.1'
	list server '/loc/10.154.7.1'
	list server '/onion/172.31.1.1#9053'

config dnsmasq 'vpn1'
	option authoritative '1'
	option dhcpscript '/usr/lib/lua/new_device.lua'
	option disabled '0'
	option domain 'vpn1'
	option domainneeded '1'
	option expandhosts '1'
	list interface 'lan_vpn1'
	option leasefile '/tmp/dhcp.leases.vpn1'
	option local '/vpn1/'
	option localise_queries '1'
	option localservice '1'
	option noresolv '1'
	list notinterface 'loopback'
	option readethers '1'
	list rebind_domain 'vpn2'
	list rebind_domain 'vpn3'
	list rebind_domain 'vpn4'
	list rebind_domain 'tor'
	list rebind_domain 'clear1'
	list rebind_domain 'clear2'
	list rebind_domain 'loc'
	option rebind_localhost '1'
	list server '/vpn2/10.154.1.1'
	list server '/vpn3/10.154.2.1'
	list server '/vpn4/10.154.3.1'
	list server '/tor/10.154.4.1'
	list server '/clear1/10.154.5.1'
	list server '/clear2/10.154.6.1'
	list server '/loc/10.154.7.1'
	list server '8.8.8.8@tun1'
	list server '8.8.4.4@tun1'
	option serversfile '/etc/dns_blacklist/lan_vpn1.overall'

