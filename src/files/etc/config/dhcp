	list server '/clear2/10.154.6.1'

config dhcp 'lan_vpn1'
	list dhcp_option 'option:classless-static-route,10.154.0.0/21,10.154.0.1'
	list dhcp_option 'option:domain-search,loc,vpn2,clear1,tor,clear2,vpn3,vpn4'
	option disabled '0'
	option force '1'
	option instance 'vpn1'
	option interface 'lan_vpn1'
	option leasetime '10m'
	option limit '251'
	option start '2'

