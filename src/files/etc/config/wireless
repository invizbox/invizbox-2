config wifi-device 'radio0'
	option channel '6'
	option disabled '0'
	option hwmode '11n'
	option htmode 'HT20'
	option noscan '1'
	option path 'platform/soc/1c1b000.usb/usb2/2-1/2-1:1.0'
	option type 'mac80211'

config wifi-device 'radio1'
	option channel '36'
	option disabled '0'
	option htmode 'VHT80'
	option hwmode '11a'
	option path 'platform/soc/1c10000.mmc/mmc_host/mmc1/mmc1:0001/mmc1:0001:1'
	option type 'mac80211'

config wifi-iface 'lan'
	option device 'radio0'
	option disabled '0'
	option encryption 'psk2+aes'
	option ifname 'ap1'
	option key 'TOKENPASSWORD'
	option mode 'ap'
	option network 'lan_vpn1'
	option ssid 'InvizBox 2 VPN'

config wifi-iface 'ap_vpn2'
	option device 'radio0'
	option disabled '1'
	option encryption 'psk2+aes'
	option ifname 'ap2'
	option key 'TOKENPASSWORD'
	option mode 'ap'
	option network 'lan_vpn2'
	option ssid ''

